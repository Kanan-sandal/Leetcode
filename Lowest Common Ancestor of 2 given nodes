class Solution {
public:
    TreeNode* lcaDeepestLeaves(TreeNode* root,TreeNode* p, TreeNode* q) {
        if(!root || root==p || root==q) return root;

        TreeNode* left=lcaDeepestLeaves(root->left,p,q);
        TreeNode* right=lcaDeepestLeaves(root->right,p,q);

        if(left && right) return root;
        return left?left:right;
    }
};
