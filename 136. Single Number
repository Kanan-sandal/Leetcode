class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int answer=0;
        for(int val:nums){
            answer^=val;
        }
        return answer;
    }
};




class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int res=0;
        for(int num:nums){
            res^=num;
        }
        return res;
    }
};
